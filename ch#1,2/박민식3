import java.util.Scanner;

public class g1152 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner input = new Scanner(System.in);
		String sen;
		sen = input.nextLine();
		int num=1;
		for (int i=1; i<sen.length(); i++) {
			if (sen.charAt(i)==' ')
				num++;
		}
		if (sen.charAt(sen.length()-1)== ' ')
			num--;
		System.out.print(num);
	}

}
